{
  "name": "luegopago-nextjs-front-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "format:dry": "eslint --fix-dry-run src/**/*.{js,jsx,ts,tsx}",
